<form action='/songs' method='post'>

  <label for="Name" id='song_name'>Name</label>
   <input type='text' name='song[name]' id='Name'><br>

  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="song[genre_ids][]" value="<%= genre.id %>"><%= genre.name%><br>
  <% end %>

  <input type='text' name='artist[name]' id="Artist Name"><br>

  <input type='submit' value='Create'>

</form>






